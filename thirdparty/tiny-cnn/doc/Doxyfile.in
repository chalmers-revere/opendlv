PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = "A header only, dependency-free deep learning framework in C++11"
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
OUTPUT_LANGUAGE        = English
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/tiny_cnn \
                         @CMAKE_CURRENT_SOURCE_DIR@/examples \
                         @CMAKE_CURRENT_SOURCE_DIR@/doc
FILE_PATTERNS          = *.h *.md
RECURSIVE              = YES
IMAGE_PATH             = @CMAKE_CURRENT_SOURCE_DIR@/doc
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/doc/readme.md
JAVADOC_AUTOBRIEF      = YES
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_SOURCE_DIR@/doc
EXCLUDE_PATTERNS       = */caffe.pb.h
